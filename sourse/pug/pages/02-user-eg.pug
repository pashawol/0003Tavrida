extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug 

block variables
	// подключи тут
	//  стили темы
	link(rel="stylesheet", href="css/eg.min.css")
	- let  mainPage= 'bg-black'
block title
	- let  title= 'Евразия'
	title= title
 
block bg
	+pic('bg-eg','bg','')(class="picture-bg")
block pagecontent 
	//- +mob-mnu()
	+top-nav('img/svg/logo-eg.svg') 
	+b.start-section
		+e.container.container
			.row.align-items-center
				.col-lg-6
					+b.user-block
						.row.align-items-center
							.col-sm-auto
								img(src="img/@2x/thumb-9.jpg", alt="")

							.col
								.h2.fw-900 Валентин 
									div Олегов
								.h5 Участник
					+b.control
						.row
							.col-sm-6
								+e.btn.btn-outline-dark(type="button") Это не я
								.small.mb-4 Нажмите, поробуем еще раз
							.col-sm-6
								+e.btn.btn-primary(type="button") Это я
								.small.mb-4 Найти все фотографии со мной
						
		+pic('main-right-img.png')(class="img-start-wrap d-none d-md-block")
				//- 	+thumbnails
		.container
			a.btn.btn-outline-light(href="#") Все фото мероприятия
	+footer()
	